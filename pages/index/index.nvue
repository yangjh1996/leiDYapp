          if (this.dataList[this.k].state == "pause") {
            uni.createVideoContext(this.dataList[this.k]._id, this).pause(); //æš‚åœä»¥åç»§ç»­æ’­æ”¾
            this.clearTime();
          }
        }
        if (this.touchNum >= 2) {
          this.doubleLike();
        }
        this.touchNum = 0;
      }, 200);
